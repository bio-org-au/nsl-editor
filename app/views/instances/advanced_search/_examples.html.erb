<div id="example-search-instance" class="example-search-tab">
<h4>Instance search examples</h4>

<h5 class="">Name queries</h5>
<table class="example-searches table table-striped">
  <% [
    {search_target: 'Instances', search_string: "Tristania",explanation: %Q(Find instances for names containing the term 'Tristania'. List the first 100.)},
    {search_target: 'Instances', search_string: "Tristania limit:200",explanation: %Q(Find instances for names containing the term 'Tristania'. List the first 200.)},
    {search_target: 'Instances', search_string: "Tristania type:basionym",explanation: %Q(Find basionym instances for names containing the term 'Tristania'.)},
      ].each do |val| %>
  <tr>
    <td class="width-30-percent">
      <a href="javascript:void(0)" 
         class="example-search width-100-percent" 
         title="Link to search">
         <span class="blue"><%= link_to(val[:search_string], search_path(query_string: val[:search_string], query_target: val[:search_target]),class:'blue', title: "Run the described search.") %></span>
      </a>
    </td>
    <td><%= val[:explanation].html_safe %></td>
  </tr>
  <% end %>
</table>
<br>

