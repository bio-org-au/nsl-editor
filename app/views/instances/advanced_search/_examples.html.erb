    {search_target: 'Instances', search_string: "page:140", explanation: %Q(instances with page 140.)},
    {search_target: 'Instances', search_string: "page:14*", explanation: %Q(instances with page 14*.)},
    {search_target: 'Instances', search_string: "page:*fig*", explanation: %Q(instances with figure information.)},
    {search_target: 'Instances', search_string: "page-qualifier:*", explanation: %Q(instances with extra page information.)},
