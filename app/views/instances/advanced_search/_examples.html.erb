<h5 class="">Count instances</h5>
<table class="example-searches table table-striped">
  <% [{search_target: 'Instances', search_string: "count",explanation: %Q(Start with "count instance" to count instances.)},
      {search_target: 'Instances', search_string: "count acacia", explanation: %Q(Counting without listing should be faster than listing for large sets.)},
      {search_target: 'Instances', search_string: "count type:basionym", explanation: %Q(Count by type e.g. basionyms.)},
      {search_target: 'Instances', search_string: "count type:comb. nov.", explanation: %Q(Count comb. novs.)},
      {search_target: 'Instances', search_string: "count acacia type:basionym", explanation: %Q(Count by name and type e.g. basionyms.)},
      {search_target: 'Instances', search_string: "count acacia type:comb. nov.", explanation: %Q(Count by name and type e.g. comb. nov.)},
      {search_target: 'Instances', search_string: "count acacia type:basionym,comb. nov.", explanation: %Q(Count by name and allow two types.)},
      {search_target: 'Instances', search_string: "count adnot:*", explanation: %Q(Count instances with an Adnot (comment).)},
      {search_target: 'Instances', search_string: "count adnot-by:*", explanation: %Q(Count instances with an Adnot (comment) by someone.)},
      {search_target: 'Instances', search_string: "count cites-an-instance:", explanation: %Q(Count instances that cite another instance.)},
      {search_target: 'Instances', 
       search_string: "count is-tax-nov-for-orth-var-name:",
       explanation: %Q(Count tax. nov. instances with an orth-var name.)},
      ].each do |val| %>
  <tr>
    <td class="width-30-percent">
      <a href="javascript:void(0)" 
         class="example-search width-100-percent" 
         title="Link to search">
         <span class="blue"><%= link_to(val[:search_string], search_path(query_string: val[:search_string], query_target: val[:search_target]),class:'blue', title: "Run the described search.") %></span>
      </a>
    </td>
    <td><%= val[:explanation].html_safe %></td>
  </tr>
  <% end %>
</table>
